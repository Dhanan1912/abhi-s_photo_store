[Unit]
Description=AbhiCloud Photos FastAPI
After=network.target


[Service]
User=ubuntu
Group=ubuntu
WorkingDirectory=/home/ubuntu/abhicloud
EnvironmentFile=/home/ubuntu/abhicloud/.env
ExecStart=/usr/bin/env uvicorn main:app --host 127.0.0.1 --port 8000 --workers 2
Restart=always


[Install]
WantedBy=multi-user.target
